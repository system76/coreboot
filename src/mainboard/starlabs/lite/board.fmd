			UEP@0x210			0x108
			# 00002000	00002FFF	00001000	OEM SMIP Partition
			SMIP@0x1000			0x1000
			# 00003000	0000DFFF	0000B000	CSE RBE Partition
			CSERBE@0x2000			0xB000
			# 0000E000	0001CFFF	0000F000	PMCP
			PMCP@0xd000			0xf000
